-Xms{{ nexus_min_heap_size }}
-Xmx{{ nexus_max_heap_size }}
-XX:MaxDirectMemorySize={{ nexus_max_direct_memory }}
-XX:+UnlockDiagnosticVMOptions
-XX:+LogVMOutput
-XX:LogFile={{ nexus_data_dir }}/nexus3/log/jvm.log
-XX:-OmitStackTraceInFastThrow
-Djava.net.preferIPv4Stack=true
-Dkaraf.home=.
-Dkaraf.base=.
-Dkaraf.etc=etc/karaf
-Djava.util.logging.config.file=etc/karaf/java.util.logging.properties
-Dkaraf.data={{ nexus_data_dir }}/nexus3
-Dkaraf.log={{ nexus_data_dir }}/nexus3/log
-Djava.io.tmpdir={{ nexus_data_dir }}/nexus3/tmp
